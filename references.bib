@article{max1993,
  title = {{Les "mondes lexicaux" et leur 'logique" \`a travers l'analyse statistique d'un corpus de r\'ecits de cauchemars}},
  author = {Max, Reinert},
  year = {1993},
  journal = {Langage et soci\'et\'e},
  volume = {66},
  number = {1},
  pages = {5--39},
  issn = {0181-4095},
  doi = {10.3406/lsoc.1993.2632},
  urldate = {2023-11-06},
  abstract = {Reinert Max - "Lexical worlds" and their "logic" through the statistical study of a body of nightmare narratives.                          The French school of data analysis, strongly influenced during the seventies by the work of J.P. Benz\'ecri, owes much to his interest in language data. The development of desk-top publishing and the spreading of texts thanks to computers have revitalized this line of research by what is usually called the "statistical analysis of textual data" (Lebart, Salem, 1988). In this article, the author presents his work and questions in the field : the Alceste method and the notion of lexical worlds (which are central to the proposed strategy). The presentation is backed up by a specific application: the analysis of a body of 212 accounts of nightmares.           ,              L'\'ecole fran\c{c}aise d'analyse des donn\'ees, fortement marqu\'ee dans les ann\'ees soixante-dix par les travaux de J.P. Benz\'ecri, doit beaucoup aux int\'et\^ets de celui-ci pour le mat\'eriel textuel. Le d\'eveloppement de la microinformatique et la diffusion des textes par ordinateur r\'eactualisent ce courant de recherche \`a travers ce qu'il est convenu d'appeler "l'analyse statistique des donn\'ees textuelles" (Lebart, Salem, 1988). Dans cet article, l'auteur pr\'esente ses travaux et ses interrogations dans ce domaine : la m\'ethodologie Alceste et la notion de mondes lexicaux (qui est au centre de la strat\'egie propos\'ee). Cette pr\'esentation est effectu\'ee \`a travers une application particuli\`ere : l'analyse d'un corpus de 212 r\'ecits de cauchemars.},
  langid = {french},
  keywords = {prospero}
}

@article{reinert1987,
  title = {Classification Ho\'erarchique Descendante et Analyse Lexicale Par Contexte. {{Application}} Au Corpus Des Po\'esies d'{{A}}. {{Rimbaud}}},
  author = {Reinert, Max},
  year = {1987},
  journal = {BMS: Bulletin of Sociological Methodology / Bulletin de M\'ethodologie Sociologique},
  number = {13},
  eprint = {43761524},
  eprinttype = {jstor},
  pages = {53--90},
  publisher = {{Sage Publications, Ltd.}},
  issn = {07591063, 20702779},
  url = {http://www.jstor.org/stable/43761524},
  urldate = {2023-11-06},
  abstract = {[Hierarchically descending classification and lexical analysis by context: application to the of A. Rimbaud's poetry. Using a lexical analysis by context and a hierarchically descending classification method, the author examines the corpus of Rimbaud's poetry. Two different codings of the corpus with the use of the same method of analysis, plus other methods also, furnish a means of judging the stability of the results obtained. Lexical analysis, Arthur Rimbaud, methodology, hierarchically descending classification analysis, coding. Employant une analyse lexicale par contexte et une m\'ethode de classification descendante hi\'erarchique, l'auteur examine le corpus de la po\'esie de Rimbaud. Deux codages diff\'erents de ce corpus et l'application de la m\^eme m\'ethode d'analyse, plus l'utilisation d'autres m\'ethodes, permettent de juger la stabilit\'e des r\'esultats obtenus. Analyae lexicale, Arthur Rimbaud, m\'ethodologie, classificaton descendante hi\'erarchique, codage.]},
  keywords = {prospero}
}

