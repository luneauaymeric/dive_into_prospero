@article{kamoun2011,
  title = {Multiple chemical sensitivity syndrome. Observation of a cohort of 20 workers},
  shorttitle = {Le syndrome d'intolérance aux odeurs chimiques. Observation d'une cohorte de 20 salariés},
  author = {Kamoun, H. and Romdhane, N.A. and Rekik, W. and Laadheri, N. and Youssef, I. and Ben Salah, F. and Gharbi, R.},
  date = {2011},
  journaltitle = {Archives des Maladies Professionnelles et de l'Environnement},
  shortjournal = {Arch. Mal. Prof. Environ.},
  volume = {72},
  number = {1},
  pages = {73--79},
  issn = {17758785 (ISSN)},
  doi = {10.1016/j.admp.2010.08.006},
  abstract = {Objectives: Multiple Chemical Sensitivity syndrome (MCS) is defined as a group of non-specific symptoms presented by a person chronically exposed to low concentrations of various chemicals. Its diagnosis and treatment are difficult. The effect of avoidance measures is uncertain. This study aims to describe MCS in occupational environment and to evaluate the effect of avoidance on its symptoms and on the future of the workers. Methods: Two successive surveys have allowed the description of MCS (20 cases) and the evaluation of avoidance recommendation (16 cases). The questionnaire is derived from the "Quick Environmental exposure and Sensitivity Inventory" (QEESI©). Diagnosis has been established according to the 1999 consensus. Median (minimum-maximum), Mann-Whitney and Kaplan-Meier survival analyses have been performed. Results: The median duration of exposure to chemical odours is 14.5 years (2-41). It is shorter (P = 0.039) for women 9.5 years (2-20) than for men 20.5 years (9-41). The median duration of MCS signs is 8.5 years (0-32). Only 44\% of workers have benefited from avoidance. Symptoms have disappeared in 31\% of the workers. The lower the initial number of MCS symptoms, the higher the disappearance frequency (P = 0.019). Early avoidance seems in favor of MCS symptoms disappearance (Log Rank P = 0.002). The probability of MCS symptoms disappearance, in the case of maintained exposure, is close to zero. In the case of avoidance, the shorter the duration of MCS signs, the higher the probability of symptoms disappearance. The probability is equal to 0.86 for a duration ≤ 1 year, to 0.57 for a duration ≤ 2 years, to 0.43 for a duration ≤ 3 years, to 0.29 for a duration ≤ 4 years and equivalent to 0 for a duration ≥ 5 years. Conclusion: Our study has verified the reality of MCS as a pathological entity. In occupational environment, early avoidance seems to constitute an effective preventive action. However, these results have to be confirmed on a larger population. © 2010 Elsevier Masson SAS. All rights reserved.},
  langid = {french},
  keywords = {Multiple chemical sensitivities (MCS),occupational exposure,Organic solvents}
}

